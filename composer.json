{
    "name": "sypspace/filament-subscriptions",
    "type": "library",
    "description": "Manage subscriptions and feature access with customizable plans in FilamentPHP",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SYPSpace\\FilamentSubscriptions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SYPSpace\\FilamentSubscriptions\\FilamentSubscriptionsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Sukmono Yogi",
            "email": "yogi@sypspace.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0",
        "laravelcm/laravel-subscriptions": "^1.3",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/filament-locations": "^2.0",
        "tomatophp/filament-translation-component": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}